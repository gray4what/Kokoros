{
  "document_groups": [
    {
      "name": "all",
      "include": [
        "**/*.py",
        "**/*.js",
        "**/*.ts",
        "**/*.md",
        "**/*.rst",
        "**/*.toml",
        "**/*.rs",
        "**/*.lock",
        "**/Dockerfile"
